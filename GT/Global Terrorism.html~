function mytest()
{




var newdata= {
newGt : []
};




for(var i in gt) {

    var item = gt[i];


if(item.country_txt=="Algeria"||item.country_txt=="Angola"||item.country_txt=="Benin"||item.country_txt=="Botswana"||item.country_txt=="Burkina"||item.country_txt=="Burundi"||item.country_txt=="Cameroon"||item.country_txt=="Cape Verde"||item.country_txt=="Central African Republic"||item.country_txt=="Chad"||item.country_txt=="Comoros"||item.country_txt=="Congo"||item.country_txt=="Congo, Democratic Republic of"||item.country_txt=="Djibouti"||item.country_txt=="Egypt"||item.country_txt=="Equatorial Guinea"||item.country_txt=="Eritrea"||item.country_txt=="Ethiopia"||item.country_txt=="Gabon"||item.country_txt=="Gambia"||item.country_txt=="Ghana"||item.country_txt=="Guinea"||item.country_txt=="Guinea-Bissau"||item.country_txt=="Ivory Coast"||item.country_txt=="Kenya"||item.country_txt=="Lesotho"||item.country_txt=="Liberia"||item.country_txt=="Libya"||item.country_txt=="Madagascar"||item.country_txt=="Malawi"||item.country_txt=="Mali"||item.country_txt=="Mauritania"||item.country_txt=="Mauritius"||item.country_txt=="Morocco"||item.country_txt=="Mozambique"||item.country_txt=="Namibia"||item.country_txt=="Niger"||item.country_txt=="Nigeria"||item.country_txt=="Rwanda"||item.country_txt=="Sao Tome and Principe"||item.country_txt=="Senegal"||item.country_txt=="Seychelles"||item.country_txt=="Sierra Leone"||item.country_txt=="Somalia"||item.country_txt=="South Africa"||item.country_txt=="South Sudan"||item.country_txt=="Sudan"||item.country_txt=="Swaziland"||item.country_txt=="Tanzania"||item.country_txt=="Togo"||item.country_txt=="Tunisia"||item.country_txt=="Uganda"||item.country_txt=="Zambia"||item.country_txt=="Zimbabwe")
africa++;

else if(item.country_txt=="Afghanistan"||item.country_txt=="Bahrain"||item.country_txt=="Bangladesh"||item.country_txt=="Bhutan"||item.country_txt=="Brunei"||item.country_txt=="Burma (Myanmar)"||item.country_txt=="Cambodia"||item.country_txt=="China"||item.country_txt=="East Timor"||item.country_txt=="India"||item.country_txt=="Indonesia"||item.country_txt=="Iran"||item.country_txt=="Iraq"||item.country_txt=="Israel"||item.country_txt=="Japan"||item.country_txt=="Jordan"||item.country_txt=="Kazakhstan"||item.country_txt=="Korea, North"||item.country_txt=="Korea, South"||item.country_txt=="Kuwait"||item.country_txt=="Kyrgyzstan"||item.country_txt=="Laos"||item.country_txt=="Lebanon"||item.country_txt=="Malaysia"||item.country_txt=="Maldives"||item.country_txt=="Mongolia"||item.country_txt=="Nepal"||item.country_txt=="Oman"||item.country_txt=="Pakistan"||item.country_txt=="Philippines"||item.country_txt=="Qatar"||item.country_txt=="Russian Federation"||item.country_txt=="Saudi Arabia"||item.country_txt=="Singapore"||item.country_txt=="Sri Lanka"||item.country_txt=="Syria"||item.country_txt=="Tajikistan"||item.country_txt=="Thailand"||item.country_txt=="Turkey"||item.country_txt=="Turkmenistan"||item.country_txt=="United Arab Emirates"||item.country_txt=="Uzbekistan"||item.country_txt=="Vietnam"||item.country_txt=="Yemen")
asia++;

else if(item.country_txt=="Albania"||item.country_txt=="Andorra"||item.country_txt=="Armenia"||item.country_txt=="Austria"||item.country_txt=="Azerbaijan"||item.country_txt=="Belarus"||item.country_txt=="Belgium"||item.country_txt=="Bosnia and Herzegovina"||item.country_txt=="Bulgaria"||item.country_txt=="Croatia"||item.country_txt=="Cyprus"||item.country_txt=="Czech Republic"||item.country_txt=="Denmark"||item.country_txt=="Estonia"||item.country_txt=="Finland"||item.country_txt=="France"||item.country_txt=="Georgia"||item.country_txt=="Germany"||item.country_txt=="Greece"||item.country_txt=="Hungary"||item.country_txt=="Iceland"||item.country_txt=="Ireland"||item.country_txt=="Italy"||item.country_txt=="Latvia"||item.country_txt=="Liechtenstein"||item.country_txt=="Lithuania"||item.country_txt=="Luxembourg"||item.country_txt=="Macedonia"||item.country_txt=="Malta"||item.country_txt=="Moldova"||item.country_txt=="Monaco"||item.country_txt=="Montenegro"||item.country_txt=="Netherlands"||item.country_txt=="Norway"||item.country_txt=="Poland"||item.country_txt=="Portugal"||item.country_txt=="Romania"||item.country_txt=="San Marino"||item.country_txt=="Serbia"||item.country_txt=="Slovakia"||item.country_txt=="Slovenia"||item.country_txt=="Spain"||item.country_txt=="Sweden"||item.country_txt=="Switzerland"||item.country_txt=="Ukraine"||item.country_txt=="United Kingdom"||item.country_txt=="Vatican City")
europe++;

else if(item.country_txt=="Antigua and Barbuda"||item.country_txt=="Bahamas"||item.country_txt=="Barbados"||item.country_txt=="Belize"||item.country_txt=="Canada"||item.country_txt=="Costa Rica"||item.country_txt=="Cuba"||item.country_txt=="Dominica"||item.country_txt=="Dominican Republic"||item.country_txt=="El Salvador"||item.country_txt=="Grenada"||item.country_txt=="Guatemala"||item.country_txt=="Haiti"||item.country_txt=="Honduras"||item.country_txt=="Jamaica"||item.country_txt=="Mexico"||item.country_txt=="Nicaragua"||item.country_txt=="Panama"||item.country_txt=="Saint Kitts and Nevis"||item.country_txt=="Saint Lucia"||item.country_txt=="Saint Vincent and the Grenadines"||item.country_txt=="Trinidad and Tobago"||item.country_txt=="United States")
na++;

else if(item.country_txt=="Australia")
au++;

else if(item.country_txt=="Argentina"||item.country_txt=="Bolivia"||item.country_txt=="Brazil"||item.country_txt=="Chile"||item.country_txt=="Colombia"||item.country_txt=="Ecuador"||item.country_txt=="Guyana"||item.country_txt=="Paraguay"||item.country_txt=="Peru"||item.country_txt=="Suriname"||item.country_txt=="Uruguay"||item.country_txt=="Venezuela")
sa++;


}


}

