document.getElementById('chart3').innerHTML = "";
document.getElementById('button').innerHTML = "";
}


function asiaRender()
{


		asiaMap();
		asiaTopCrimeCountry();
    		document.getElementById('mylocation1').innerHTML="Details of Asia";
		document.getElementById('mylocation2').innerHTML="Details of Asia";
		document.getElementById('chart2').innerHTML = "";
		document.getElementById('chart3').innerHTML = "";
		document.getElementById('button').innerHTML = "<input type='submit' value='World Map' onclick='homeButton();'/>";
}





